@model List<BildstudionDV.BI.ViewModels.UserProfileViewModel>


<form action="~/acount/adduser">
    <button class="btn-outline-success btn-block btn nav-link">Lägg till ny Användare</button>
</form>
<table class="table table-striped table-bordered">
    <thead>
        <tr style="background-color: darkgray;color:black;">
            <th>Användarens Namn</th>
            <th>Gruppen Tillhör</th>
            <th>Redigera Användaren</th>
            <th>Ta Bort Användaren</th>

        </tr>
    </thead>
    @foreach (var userModel in Model)
    {
        if (userModel.UserName == "admin" || userModel.UserName == "piahag")
        { }
        else
        {
<tr style="color:white;">
    <td>@userModel.UserName</td>
    <td>@userModel.AssociatedGrupp</td>
    <td>
        <input type="button" class="btn btn-info" onclick='window.location.href = "../acount/edituser?namn=@userModel.UserName"' value="Redigera Användaren" />
    </td>
    <td>
        <input type="button" class="btn btn-danger" onclick='window.location.href = "../acount/removeuser?namn=@userModel.UserName"' value="Ta Bort Användaren" />
    </td>
</tr>
        }
    }
    <tfoot>
        <tr style="background-color: darkgray;color:black;">
            <th>Användarens Namn</th>
            <th>Gruppen Tillhör</th>
            <th>Redigera Användaren</th>
            <th>Ta Bort Användaren</th>
        </tr>
    </tfoot>
</table>